{
  "name": "runty",
  "author": "_nderscore",
  "version": "0.1.7",
  "description": "Extensible conditional string micro templates",
  "keywords": [
    "string",
    "template",
    "interpolation",
    "substitution",
    "expansion"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:mjs && yarn build:cjs",
    "build:cjs": "babel --delete-dir-on-start --out-dir dist/cjs --env-name cjs ./src",
    "build:mjs": "babel --delete-dir-on-start --out-dir dist/mjs ./src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "homepage": "https://runty.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/nderscore/runty"
  },
  "engines": {
    "node": ">= 14"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1"
  },
  "packageManager": "yarn@3.1.1"
}
